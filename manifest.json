{
    "manifest_version": 3,
    "name": "Let - URL Shortener",
    "version": "0.1",
    "description": "Create a tiny URL",
    "icons": {
      "128": "logo.png"
    },
    "action": {
      "default_popup": "popup.html",
      "default_icon": "logo.png"
    },
    "options_page": "options.html",
    "permissions": [
      "storage"
    ]
  }